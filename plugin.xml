<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="STView"
            id="STView">
      </category>
      <view
            name="TreeView"
            icon="icons/View.gif"
            category="STView"
            class="stview.views.STView"
            id="stview.views.STView">
      </view>
      <view
            category="STView"
            class="stview.profiler.dprofiler.ProfilerView"
            icon="icons/View.gif"
            id="stview.profiler.ProfilerView"
            name="ProfilerView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="stview.views.STView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="stview.preferences.STVPreferencePage"
            id="stview"
            name="STView">
      </page>
      <page
            category="stview"
            class="stview.preferences.FilePreferencePage"
            id="stview.file"
            name="File">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="stview.preferences.PreferenceConstants">
      </initializer>
   </extension>

</plugin>
